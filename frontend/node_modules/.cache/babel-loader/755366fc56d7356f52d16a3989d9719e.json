{"ast":null,"code":"import \"../Firebase\";\nimport { getAuth, signInWithEmailAndPassword } from \"firebase/auth\";\nconst auth = getAuth();\nimport router from \"../router\";\nexport default {\n  name: \"LoginView\",\n\n  data() {\n    return {\n      user: {\n        email: \"\",\n        password: \"\"\n      },\n      messageError: \"\"\n    };\n  },\n\n  methods: {\n    loginUser() {\n      signInWithEmailAndPassword(auth, this.user.email, this.user.password).then(dataUser => {\n        localStorage.setItem(\"uidUser\", dataUser.user.uid);\n\n        if (this.$store.state.count > 0) {\n          router.push(\"/cart\");\n        } else {\n          router.push(\"/products\");\n        }\n      }).catch(() => {\n        this.messageError = \"invalid email and password\";\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqEA,OAAO,aAAP;AACA,SAASA,OAAT,EAAkBC,0BAAlB,QAAoD,eAApD;AACA,MAAMC,IAAG,GAAIF,OAAO,EAApB;AACA,OAAOG,MAAP,MAAmB,WAAnB;AACA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE,EADH;QAEJC,QAAQ,EAAE;MAFN,CADD;MAKLC,YAAY,EAAE;IALT,CAAP;EAOD,CAVY;;EAWbC,OAAO,EAAE;IACPC,SAAS,GAAG;MACVV,0BAA0B,CAACC,IAAD,EAAO,KAAKI,IAAL,CAAUC,KAAjB,EAAwB,KAAKD,IAAL,CAAUE,QAAlC,CAA1B,CACGI,IADH,CACSC,QAAD,IAAc;QAClBC,YAAY,CAACC,OAAb,CAAqB,SAArB,EAAgCF,QAAQ,CAACP,IAAT,CAAcU,GAA9C;;QACA,IAAI,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,KAAlB,GAA0B,CAA9B,EAAiC;UAC/BhB,MAAM,CAACiB,IAAP,CAAY,OAAZ;QACF,CAFA,MAEO;UACLjB,MAAM,CAACiB,IAAP,CAAY,WAAZ;QACF;MACD,CARH,EASGC,KATH,CASS,MAAM;QACX,KAAKZ,YAAL,GAAoB,4BAApB;MACD,CAXH;IAYD;;EAdM;AAXI,CAAf","names":["getAuth","signInWithEmailAndPassword","auth","router","name","data","user","email","password","messageError","methods","loginUser","then","dataUser","localStorage","setItem","uid","$store","state","count","push","catch"],"sourceRoot":"","sources":["C:\\Users\\qimeng\\Downloads\\Online-Store-main (1)\\Online-Store-main\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\n  <div\n    class=\"vh-100 d-flex justify-content-center align-items-center\"\n    style=\"background: rgb(57 181 255)\"\n  >\n    <div class=\"container\">\n      <div class=\"row d-flex justify-content-center\">\n        <div class=\"col-12 col-md-8 col-lg-6\">\n          <div class=\"card bg-white\">\n            <div class=\"card-body p-5\">\n              <form class=\"mb-3 mt-md-4\" @submit.prevent=\"loginUser\">\n                <h2\n                  class=\"fw-bold mb-2 text-uppercase\"\n                  style=\"margin-left: 29%; padding-bottom: 22px\"\n                >\n                  Welcome !\n                </h2>\n                <!-- <p class=\"mb-5\">Please enter your login and password!</p> -->\n                <div class=\"alert alert-danger\" v-if=\"messageError\">\n                  {{ messageError }}\n                </div>\n                <div class=\"mb-3\">\n                  <label for=\"email\" class=\"form-label\">Email address</label>\n                  <input\n                    type=\"email\"\n                    class=\"form-control\"\n                    id=\"email\"\n                    placeholder=\"name@example.com\"\n                    v-model=\"user.email\"\n                  />\n                </div>\n                <div class=\"mb-3\">\n                  <label for=\"password\" class=\"form-label\">Password</label>\n                  <input\n                    type=\"password\"\n                    class=\"form-control\"\n                    id=\"password\"\n                    placeholder=\"*******\"\n                    v-model=\"user.password\"\n                  />\n                </div>\n                <!-- <p class=\"small\">\n                  <a class=\"text-primary\" href=\"forget-password.html\"\n                    >Forgot password?</a\n                  >\n                </p> -->\n                <div class=\"d-grid\">\n                  <button class=\"btn btn-outline-dark\" type=\"submit\">\n                    Login\n                  </button>\n                </div>\n              </form>\n              <div>\n                <p class=\"mb-0 text-center\">\n                  Don't have an account?\n                  <router-link to=\"/signin\" class=\"text-primary fw-bold\"\n                    >Sign Up</router-link\n                  >\n                </p>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport \"../Firebase\";\nimport { getAuth, signInWithEmailAndPassword } from \"firebase/auth\";\nconst auth = getAuth();\nimport router from \"../router\";\nexport default {\n  name: \"LoginView\",\n  data() {\n    return {\n      user: {\n        email: \"\",\n        password: \"\",\n      },\n      messageError: \"\",\n    };\n  },\n  methods: {\n    loginUser() {\n      signInWithEmailAndPassword(auth, this.user.email, this.user.password)\n        .then((dataUser) => {\n          localStorage.setItem(\"uidUser\", dataUser.user.uid);\n          if (this.$store.state.count > 0) {\n            router.push(\"/cart\");\n          } else {\n            router.push(\"/products\");\n          }\n        })\n        .catch(() => {\n          this.messageError = \"invalid email and password\";\n        });\n    },\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module"}