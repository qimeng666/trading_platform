{"ast":null,"code":"import \"../Firebase\";\nimport { getAuth, signInWithEmailAndPassword } from \"firebase/auth\";\nconst auth = getAuth();\nimport router from \"../router\";\nexport default {\n  name: \"LoginView\",\n  data() {\n    return {\n      user: {\n        email: \"\",\n        password: \"\"\n      },\n      messageError: \"\"\n    };\n  },\n  methods: {\n    loginUser() {\n      signInWithEmailAndPassword(auth, this.user.email, this.user.password).then(dataUser => {\n        localStorage.setItem(\"uidUser\", dataUser.user.uid);\n        if (this.$store.state.count > 0) {\n          router.push(\"/cart\");\n        } else {\n          router.push(\"/products\");\n        }\n      }).catch(() => {\n        this.messageError = \"invalid email and password\";\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAqEA,OAAO,aAAa;AACpB,SAASA,OAAO,EAAEC,0BAAyB,QAAS,eAAe;AACnE,MAAMC,IAAG,GAAIF,OAAO,CAAC,CAAC;AACtB,OAAOG,MAAK,MAAO,WAAW;AAC9B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ,CAAC;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,GAAG;MACVV,0BAA0B,CAACC,IAAI,EAAE,IAAI,CAACI,IAAI,CAACC,KAAK,EAAE,IAAI,CAACD,IAAI,CAACE,QAAQ,EACjEI,IAAI,CAAEC,QAAQ,IAAK;QAClBC,YAAY,CAACC,OAAO,CAAC,SAAS,EAAEF,QAAQ,CAACP,IAAI,CAACU,GAAG,CAAC;QAClD,IAAI,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,KAAI,GAAI,CAAC,EAAE;UAC/BhB,MAAM,CAACiB,IAAI,CAAC,OAAO,CAAC;QACtB,OAAO;UACLjB,MAAM,CAACiB,IAAI,CAAC,WAAW,CAAC;QAC1B;MACF,CAAC,EACAC,KAAK,CAAC,MAAM;QACX,IAAI,CAACZ,YAAW,GAAI,4BAA4B;MAClD,CAAC,CAAC;IACN;EACF;AACF,CAAC","names":["getAuth","signInWithEmailAndPassword","auth","router","name","data","user","email","password","messageError","methods","loginUser","then","dataUser","localStorage","setItem","uid","$store","state","count","push","catch"],"sourceRoot":"","sources":["C:\\Users\\qimeng\\Documents\\GitHub\\trading_platform\\frontend\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\n  <div\n    class=\"vh-100 d-flex justify-content-center align-items-center\"\n    style=\"background: rgb(57 181 255)\"\n  >\n    <div class=\"container\">\n      <div class=\"row d-flex justify-content-center\">\n        <div class=\"col-12 col-md-8 col-lg-6\">\n          <div class=\"card bg-white\">\n            <div class=\"card-body p-5\">\n              <form class=\"mb-3 mt-md-4\" @submit.prevent=\"loginUser\">\n                <h2\n                  class=\"fw-bold mb-2 text-uppercase\"\n                  style=\"margin-left: 29%; padding-bottom: 22px\"\n                >\n                  Welcome !\n                </h2>\n                <!-- <p class=\"mb-5\">Please enter your login and password!</p> -->\n                <div class=\"alert alert-danger\" v-if=\"messageError\">\n                  {{ messageError }}\n                </div>\n                <div class=\"mb-3\">\n                  <label for=\"email\" class=\"form-label\">Email address</label>\n                  <input\n                    type=\"email\"\n                    class=\"form-control\"\n                    id=\"email\"\n                    placeholder=\"name@example.com\"\n                    v-model=\"user.email\"\n                  />\n                </div>\n                <div class=\"mb-3\">\n                  <label for=\"password\" class=\"form-label\">Password</label>\n                  <input\n                    type=\"password\"\n                    class=\"form-control\"\n                    id=\"password\"\n                    placeholder=\"*******\"\n                    v-model=\"user.password\"\n                  />\n                </div>\n                <!-- <p class=\"small\">\n                  <a class=\"text-primary\" href=\"forget-password.html\"\n                    >Forgot password?</a\n                  >\n                </p> -->\n                <div class=\"d-grid\">\n                  <button class=\"btn btn-outline-dark\" type=\"submit\">\n                    Login\n                  </button>\n                </div>\n              </form>\n              <div>\n                <p class=\"mb-0 text-center\">\n                  Don't have an account?\n                  <router-link to=\"/signin\" class=\"text-primary fw-bold\"\n                    >Sign Up</router-link\n                  >\n                </p>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport \"../Firebase\";\nimport { getAuth, signInWithEmailAndPassword } from \"firebase/auth\";\nconst auth = getAuth();\nimport router from \"../router\";\nexport default {\n  name: \"LoginView\",\n  data() {\n    return {\n      user: {\n        email: \"\",\n        password: \"\",\n      },\n      messageError: \"\",\n    };\n  },\n  methods: {\n    loginUser() {\n      signInWithEmailAndPassword(auth, this.user.email, this.user.password)\n        .then((dataUser) => {\n          localStorage.setItem(\"uidUser\", dataUser.user.uid);\n          if (this.$store.state.count > 0) {\n            router.push(\"/cart\");\n          } else {\n            router.push(\"/products\");\n          }\n        })\n        .catch(() => {\n          this.messageError = \"invalid email and password\";\n        });\n    },\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module"}